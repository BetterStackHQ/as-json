{
  "name": "json-as",
  "version": "0.5.50",
  "description": "JSON encoder/decoder for AssemblyScript",
  "types": "assembly/index.ts",
  "author": "Jairus Tanaka",
  "contributors": [
    "DogWhich",
    "Romdotdog",
    "Derek Barrera",
    "Frankk Taylor",
    "lekiano"
  ],
  "license": "MIT",
  "scripts": {
    "aspect": "asp",
    "bench:astral": "astral -Ospeed --noAssert --uncheckedBehavior always --runtime minimal",
    "build:test": "asc assembly/test.ts --target test --runtime stub",
    "build:transform": "tsc -p ./transform",
    "test:wasmtime": "wasmtime ./build/test.wasm",
    "test:lunatic": "lunatic ./build/test.wasm",
    "test:wasm3": "wasm3 ./build/test.wasm",
    "prettier": "as-prettier -w ."
  },
  "devDependencies": {
    "@as-pect/cli": "^8.0.1",
    "@as-tral/cli": "^2.0.0",
    "@assemblyscript/wasi-shim": "^0.1.0",
    "assemblyscript": "^0.27.1",
    "assemblyscript-prettier": "^1.0.7",
    "prettier": "^2.8.4",
    "tinybench": "^2.5.0",
    "typescript": "^4.9.5",
    "visitor-as": "^0.11.4"
  },
  "dependencies": {
    "as-string-sink": "^0.5.3",
    "as-variant": "^0.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JairusSW/as-json.git"
  },
  "keywords": [
    "assemblyscript",
    "json",
    "serialize",
    "deserialize",
    "dynamic",
    "serde"
  ],
  "bugs": {
    "url": "https://github.com/JairusSW/as-json/issues"
  },
  "homepage": "https://github.com/JairusSW/as-json#readme",
